@INPROCEEDINGS{Chu:ICDC08,
  author = {Chu, Wei-rong and Chen, Yu-shin and Hsu, Jane Yung-jen and Su, Yi-Jen
  and Chen, Sue-Huei},
  title = {Emotion Recognition System Using Physiological Signals: Experiment
  and Application},
  booktitle = {Proceedings of the 1st International Conference on Digital Content},
  year = {2008},
  address = {Taoyuan, Taiwan},
  month = {December},
  abstract = {The recognition of emotion states is widely developed for the field
  of human-computerinteraction. In this paper, firstly, we develop
  the system for emotion recognitionbased on physiological signals,
  including skin temperature, skin conductance, bloodvolume pressure
  and respiration. For collecting physiological signal, subjects areconducted
  by mood induction video to reach the specific emotion state. The
  recognition system consists of emotion induction, feature extraction,
  and pattern classification stages.We adopt support vector machine
  to emotion classification problem. The resultsachieve high accuracy
  about 90%.Secondly, to minimize the number of sensors in experiments,
  we measure theimportance of each sensor by employing part of four
  sensors. Thirdly, we use thesignal of ``peace'' state as baseline
  value to calibrate all physiological signals.Finally, we apply calibration
  method to unseen data collected in another new day.The classification
  process is similar with real-time recognition. The result shows there
  is significant improvement in accuracy after calibration.For the
  application of emotion recognition system, we construct a prototype
  of emotion-aware music player. The system selects music dynamically
  according to user's emotion states. In the future, we can add more
  function in this prototype.},
  keywords = {Emotion Recognition, Physiological Sensor, Support Vector Machine},
  timestamp = {2008.12.18},
  url = {http://icdc2008.ci.yzu.edu.tw/?p=abstract&paper_id=165}
}

@ARTICLE{Chu:ACMMM05,
  author = {Chu, Wei-Ta and Cheng, Wen-Huang and Hsu, Jane Yung-jen and Wu, Ja-Ling},
  title = {Toward semantic indexing and retrieval using hierarchical audio models},
  journal = {ACM Multimedia Systems Journal},
  year = {2005},
  volume = {10},
  pages = {570--583},
  number = {6},
  month = {October},
  note = {SCI},
  abstract = {Semantic-level content analysis is a crucial issue in achieving efficient
  content retrieval and management. We propose a hierarchical approach
  that models the statistical characteristics of audio events over
  a time series to accomplish semantic context detection. Two stages,
  audio event and semantic context modeling, are devised to bridge
  the semantic gap between physical audio features and semantic concepts.
  In this work, hidden Markov models (HMMs) are used to model four
  representative audio events, i.e., gunshot, explosion, engine, and
  car-braking, in action movies. At the semantic-context level, Gaussian
  mixture models (GMMs) and ergodic HMMs are investigated to fuse the
  characteristics and correlations between various audio events. They
  provide cues for detecting gunplay and car-chasing scenes, two semantic
  contexts we focus on in this work. The promising experimental results
  demonstrate the effectiveness of the proposed approach and exhibit
  that the proposed framework provides a foundation in semantic indexingand
  retrieval. Moreover, the two fusion schemes are compared, and the
  relations between audio event and semantic context are studied.},
  doi = {10.1007/s00530-005-0183-6},
  keywords = {audio event, semantic context, semantic gap, hidden markov model,
  gaussian mixture model},
  url = {http://www.springerlink.com/content/t081634457t8h664/?p=1acbf32c38f343ba8577519893354f28&pi=9}
}


@INPROCEEDINGS{Chang:UbiMedia08,
  author = {Chang, Yu-Hsin and Chan, Li-Wei and Ko, Ju-Chun and Lee, Ming-Sui
  and Hsu, Jane Yung-jen and Hung, Yi-Ping},
  title = {{QPalm}: A Gesture Recognition System for Remote Control with List
